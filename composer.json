{
  "name": "vahidkaargar/laravel-departments",
  "description": "This is a package for managing your departments in your application easy and beautiful",
  "type": "library",
  "keywords": [
    "laravel",
    "departments_system",
    "package"
  ],
  "license": "MIT",
  "authors": [
    {
      "email": "vahidkaargar@gmail.com",
      "name": "Vahid Kaargar"
    }
  ],
  "autoload": {
    "psr-4": {
      "vahidkaargar\\LaravelDepartments\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4",
    "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
    "josantonius/http-status-code": "^1.0|^2.0",
    "guzzlehttp/guzzle": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "orchestra/testbench": "^8.10"
  },
  "autoload-dev": {
    "psr-4": {
      "vahidkaargar\\LaravelDepartments\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "vahidkaargar\\LaravelDepartments\\ServiceProviders\\DepartmentsServiceProvider"
      ]
    }
  }
}
