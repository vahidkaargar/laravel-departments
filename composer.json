{
  "name": "vahidkaargar/laravel-departments",
  "description": "This is a package for managing your departments in your application easy and beautiful",
  "type": "library",
  "keywords": [
    "laravel",
    "departments",
    "package"
  ],
  "license": "MIT",
  "authors": [
    {
      "email": "vahidkaargar@gmail.com",
      "name": "Vahid Kaargar"
    },
    {
      "email": "momenih333@gmail.com",
      "name": "Hamidreza Momeni"
    }
  ],
  "autoload": {
    "psr-4": {
      "vahidkaargar\\LaravelDepartments\\": "src/"
    },
    "files": [
      "src/Helpers/helpers.php"
    ]
  },
  "require": {
    "php": ">=8.0",
    "josantonius/http-status-code": "^1.0|^2.0",
    "guzzlehttp/guzzle": "^7.2",
    "illuminate/database": "^v11.33.2",
    "illuminate/support": "^v11.33.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.4.",
    "orchestra/testbench": "^v9.1"
  },
  "autoload-dev": {
    "psr-4": {
      "vahidkaargar\\LaravelDepartments\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "vahidkaargar\\LaravelDepartments\\ServiceProviders\\DepartmentsServiceProvider"
      ],
      "aliases": {
        "Departments": "Departments"
      }
    }
  }
}
